{
  "name": "nuxt-app",
  "type": "module",
  "private": true,
  "scripts": {
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx,.vue --cache --max-warnings=0",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "prepare": "husky",
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,vue}": [
      "eslint --fix --cache",
      "prettier --write"
    ],
    "*.{json,md,css,scss,html,yml,yaml}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@pinia/nuxt": "^0.11.2",
    "apexcharts": "^5.3.5",
    "chart.js": "^4.5.1",
    "chartjs-plugin-datalabels": "^2.2.0",
    "nuxt": "^4.1.1",
    "pinia": "^3.0.3",
    "vite-tsconfig-paths": "^5.1.4",
    "vue": "^3.5.21",
    "vue-chartjs": "^5.3.2",
    "vue-router": "^4.5.1",
    "vue3-apexcharts": "^1.9.0"
  },
  "devDependencies": {
    "@mdi/font": "^7.4.47",
    "@nuxtjs/tailwindcss": "^6.14.0",
    "@types/node": "^24.5.2",
    "@typescript-eslint/eslint-plugin": "^8.45.0",
    "@typescript-eslint/parser": "^8.45.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-vue": "^10.5.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.3",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "sass": "^1.92.1",
    "tailwindcss": "^3.4.17",
    "vue-eslint-parser": "^10.2.0",
    "vuetify": "^3.10.0",
    "vuetify-nuxt-module": "^0.18.7"
  }
}
